{
  "$schema": "./_buildtools/targets.spec.json",
  "rust": {
    "location": "rust",
    "build": "cargo build",
    "test": "cargo test"
  },
  "javascript": {
    "location": "javascript",
    "setup": "yarn",
    "build": "yarn build",
    "test": "yarn test"
  },
  "dart": {
    "location": "dart",
    "setup": "dart pub get",
    "build": "dart compile exe example/dtnb_example.dart",
    "test": "dart test"
  },
  "golang": {
    "location": "golang",
    "build": "go build",
    "test": "go test"
  }
}
